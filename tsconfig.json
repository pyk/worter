{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "ES2019",
        "lib": ["DOM", "DOM.Iterable", "WebWorker", "ES2019"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": ["@cloudflare/workers-types"],
        "skipLibCheck": true
    },
    "include": ["index.ts"],
    "exclude": []
}
